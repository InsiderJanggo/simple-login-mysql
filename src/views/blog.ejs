<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
        <% blog.forEach(function(data) { %>
            <% if(data.length == 0) { %>
                <h1>No data was found</h1>
            <% } else { %>
                <%= data.title %>
            <% } %>
        <% })%>
</body>
</html>